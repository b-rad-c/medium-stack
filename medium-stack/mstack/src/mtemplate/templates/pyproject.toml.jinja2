[build-system]
requires = ['setuptools>=61.0']
build-backend = 'setuptools.build_meta'

[project]
name = '{{ package_name }}'
version = '0.0.1'
authors = [
  { name='Brad Corlett', email='brad.corlett@gmail.com' },
]
description = 'A project generated by mstack'
readme = 'README.md'
requires-python = '>=3.7'
dependencies = [
    'mstack'
]
